{
  "name": "file-management-system-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "export NODE_ENV=development && sequelize-cli db:migrate",
    "migrate:undo": "export NODE_ENV=development && sequelize-cli db:migrate:undo",
    "migrate:undo:all": "export NODE_ENV=development && sequelize-cli db:migrate:undo:all",
    "seed": "export NODE_ENV=development && sequelize-cli db:seed:all",
    "seed:undo": "export NODE_ENV=development && sequelize-cli db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "sequelize": "^6.37.1",
    "sequelize-hierarchy": "^2.0.4",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2"
  }
}
